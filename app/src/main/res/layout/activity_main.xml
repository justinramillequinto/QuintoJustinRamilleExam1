        android:layout_marginBottom="8dp"
        android:background="#FFFF00"
        android:text="Note 1\nAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAABBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBB"
