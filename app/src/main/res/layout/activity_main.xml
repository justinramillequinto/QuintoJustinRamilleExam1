        android:layout_marginRight="8dp"
        android:background="#FF0000"
        android:text="Note 1\nAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
