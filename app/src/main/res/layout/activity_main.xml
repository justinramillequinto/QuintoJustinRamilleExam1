        android:layout_width="395dp"
        android:layout_height="1dp"
        android:background="#FFFFFF"
        android:visibility="visible"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="63dp" />

    <ImageView
        android:id="@+id/imageView4"
